{
  "name": "@adoniscommerce/template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "description": "AdonisCommerce E-Commerce Template",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "test:e2e": "jest --config tests/e2e/jest.config.js",
    "test:e2e:headed": "HEADLESS=false jest --config tests/e2e/jest.config.js",
    "test:e2e:debug": "HEADLESS=false SLOW_MO=100 jest --config tests/e2e/jest.config.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "infra:up": "DB_PORT=5440 REDIS_PORT=6390 docker compose up -d postgres redis",
    "infra:down": "docker compose down",
    "infra:status": "docker compose ps",
    "infra:logs": "docker compose logs -f",
    "db:migrate": "node ace migration:run",
    "db:seed": "node ace db:seed",
    "db:reset": "node ace migration:rollback --batch=0 && node ace migration:run && node ace db:seed",
    "db:fresh": "node ace migration:fresh && node ace db:seed",
    "queue:work": "node ace commerce:queue:work --scheduled",
    "queue:status": "node ace commerce:queue:status",
    "setup": "pnpm infra:up && sleep 3 && pnpm db:migrate && pnpm db:seed && echo '✅ Kurulum tamamlandı!'",
    "go": "pnpm infra:up && sleep 2 && pnpm dev",
    "docker:dev": "./scripts/docker-start.sh dev",
    "docker:dev:tools": "./scripts/docker-start.sh dev:tools",
    "docker:prod": "./scripts/docker-start.sh prod",
    "docker:stop": "./scripts/docker-start.sh stop",
    "docker:logs": "./scripts/docker-start.sh logs",
    "docker:shell": "./scripts/docker-start.sh shell",
    "docker:db:migrate": "./scripts/docker-start.sh db:migrate",
    "docker:db:seed": "./scripts/docker-start.sh db:seed",
    "docker:db:reset": "./scripts/docker-start.sh db:reset",
    "docker:clean": "./scripts/docker-start.sh clean",
    "docker:status": "./scripts/docker-start.sh status"
  },
  "imports": {
    "#contracts/*": "./app/contracts/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#helpers/*": "./app/helpers/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@tailwindcss/vite": "^4.1.18",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "puppeteer": "^23.11.1",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.2.5",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.8",
    "vite": "^6.3.5",
    "youch": "^3.3.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/inertia": "^3.1.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/redis": "^9.1.0",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.2.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@inertiajs/react": "^2.3.13",
    "@puckeditor/core": "^0.21.1",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "@vinejs/vine": "^3.0.1",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "edge.js": "^6.2.1",
    "framer-motion": "^11.18.0",
    "lucide-react": "^0.563.0",
    "luxon": "^3.7.2",
    "stripe": "^17.5.0",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^2.5.4",
    "tw-animate-css": "^1.4.0",
    "bullmq": "^5.34.0",
    "meilisearch": "^0.45.0",
    "otpauth": "^9.3.5",
    "zustand": "^5.0.11"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts",
      "./app/jobs/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
